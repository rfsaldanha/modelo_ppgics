issn = {00348910},
journal = {Revista de Saude Publica},
keywords = {Health care quality,Access,Evaluation,Integrality in health,Uterine cervical neoplasms,Prevention {\&} control,Women's health services},
number = {2},
pages = {240--248},
pmid = {24897045},
title = {{Integrality in cervical cancer care: Evaluation of access}},
volume = {48},
year = {2014}
}
