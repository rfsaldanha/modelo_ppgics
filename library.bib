journal = {Revista panamericana de salud p{\'{u}}blica},
number = {1},
pages = {1--13},
pmid = {24006014},
title = {{Comparison of national health research priority-setting methods and characteristics in Latin America and the Caribbean, 2002-2012.}},
volume = {34},
