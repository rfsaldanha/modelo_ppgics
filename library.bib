title = {{Geoprocessamento, ambiente e sa{\'{u}}de: uma uni{\~{a}}o poss{\'{i}}vel?}},
url = {http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S0102-311X1996000300012{\&}lng=pt{\&}tlng=pt},
