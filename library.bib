issn = {1879-2057},
journal = {Accident; analysis and prevention},
keywords = {Accidents, Traffic,Accidents, Traffic: mortality,Accidents, Traffic: trends,Developed Countries,Developed Countries: statistics {\&} numerical data,Economic Development,Economic Development: statistics {\&} numerical data,Humans},
month = {mar},
