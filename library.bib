@article{Martins2004,
abstract = {Desde suas origens, a ci{\^{e}}ncia da informa{\c{c}}{\~{a}}o no Brasil esteve estreitamente vinculada ao sistema governamental de C{\&}T, o que implicou sua estreita rela{\c{c}}{\~{a}}o com as pol{\'{i}}ticas de Estado. As mudan{\c{c}}as pol{\'{i}}ticas e ideol{\'{o}}gicas por que passou o pa{\'{i}}s nos {\'{u}}ltimos 50 anos influ{\'{i}}ram de forma marcante nos caminhos percorridos pela CI. A discuss{\~{a}}o de como as ideologias e as pol{\'{i}}ticas p{\'{u}}blicas influenciaram a CI, a partir da an{\'{a}}lise do Ibict, pode fornecer instrumentos para entender o momento atual e pensar o futuro da CI no pa{\'{i}}s.},
