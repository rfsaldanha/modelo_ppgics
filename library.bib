@article{Luke2012,
abstract = {Complex systems abound in public health. Complex systems are made up of heterogeneous elements that interact with one another, have emergent properties that are not explained by understanding the individual elements of the system, persist over time, and adapt to changing circumstances. Public health is starting to use results from systems science studies to shape practice and policy, for example in preparing for global pandemics. However, systems science study designs and analytic methods remain underutilized and are not widely featured in public health curricula or training. In this review we present an argument for the utility of systems science methods in public health, introduce three important systems science methods (system dynamics, network analysis, and agent-based modeling), and provide three case studies in which these methods have been used to answer important public health science questions in the areas of infectious disease, tobacco control, and obesity.},
author = {Luke, Douglas A. Da and Stamatakis, Katherine A.},
doi = {10.1146/annurev-publhealth-031210-101222.Systems},
file = {:Users/raphaelsaldanha/Library/Application Support/Mendeley Desktop/Downloaded/Luke, Stamatakis - 2012 - System Science Methods in Public Health Dynamics, Networks, and Agents.pdf:pdf},
isbn = {0163-7525},
issn = {1545-2093},
journal = {Annual review of public health},
keywords = {agent-based modeling,complex systems,computer models,network analysis,system dynamics},
number = {1},
pages = {357--376},
pmid = {22224885},
title = {{System Science Methods in Public Health: Dynamics, Networks, and Agents}},
