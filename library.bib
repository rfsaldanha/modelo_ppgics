@article{Becerra-Posada2014,
abstract = {BACKGROUND:$\backslash$r$\backslash$n$\backslash$r$\backslash$nNational Research for Health Systems (NRfHS) in Latin America and the Caribbean (LAC) have shown growth and consolidation in the last few years. A structured, organized system will facilitate the development and implementation of strategies for research for health to grow and contribute towards people's health and equity.$\backslash$r$\backslash$n$\backslash$r$\backslash$nMETHODS:$\backslash$r$\backslash$nWe conducted a survey with the health managers from LAC countries that form part of the Ibero-American Ministerial Network for Health Education and Research.$\backslash$r$\backslash$n$\backslash$r$\backslash$nRESULTS:$\backslash$r$\backslash$nFrom 13 of 18 questionnaires delivered, we obtained information on the NRfHS governance and management structures, the legal and political framework, the research priorities, existing financing schemes, and the main institutional actors. Data on investment in science and technology, scientific production, and on the socio-economic reality of countries were obtained through desk review focused on regional/global data sources to increase comparability.$\backslash$r$\backslash$n$\backslash$r$\backslash$nCONCLUSIONS:$\backslash$r$\backslash$nBy comparing the data gathered with a review carried out in 2008, we were able to document the advances in research for health system development in the region, mostly in setting governance, coordination, policies, and regulations, key for better functionality of research for health systems. However, in spite of these advances, growth and consolidation of research for health systems in the region is still uneven.},
author = {Becerra-Posada, Francisco and Minayo, Miryam and Quental, Cristiane and de Haan, Sylvia},
doi = {10.1186/1478-4505-12-13},
file = {:Users/raphaelsaldanha/Library/Application Support/Mendeley Desktop/Downloaded/Becerra-Posada et al. - 2014 - National research for health systems in Latin America and the Caribbean moving towards the right directio.pdf:pdf},
isbn = {1478-4505},
issn = {1478-4505},
journal = {Health Research Policy and Systems},
keywords = {health research,latin america and caribbean,national research for health,systems},
month = {dec},
number = {1},
pages = {13},
pmid = {24602201},
title = {{National research for health systems in Latin America and the Caribbean: moving towards the right direction?}},
url = {http://health-policy-systems.biomedcentral.com/articles/10.1186/1478-4505-12-13},
volume = {12},
year = {2014}
}
