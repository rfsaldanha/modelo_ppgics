@article{Stern2004,
abstract = {This paper presents a critical history of the environmental Kuznets curve (EKC). The EKC proposes that indicators of environmental degradation first rise, and then fall with increasing income per capita. Recent evidence shows however, that developing countries are addressing environmental issues, sometimes adopting developed country standards with a short time lag and sometimes performing better than some wealthy countries, and that the EKC results have a very flimsy statistical foundation. A new generation of decomposition and efficient frontier models can help disentangle the true relations between development and the environment and may lead to the demise of the classic EKC.},
author = {Stern, David I},
doi = {10.1016/j.worlddev.2004.03.004},
file = {:Users/raphaelsaldanha/Library/Application Support/Mendeley Desktop/Downloaded/Stern - 2004 - The Rise and Fall of the Environmental Kuznets Curve.pdf:pdf},
