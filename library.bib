doi = {10.1016/S0140-6736(18)31221-2},
file = {:Users/raphaelsaldanha/Library/Application Support/Mendeley Desktop/Downloaded/Marinho et al. - 2018 - Burden of disease in Brazil, 1990–2016 a systematic subnational analysis for the Global Burden of Disease Stud.pdf:pdf},
