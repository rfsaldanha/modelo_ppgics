@article{Brasil2016,
abstract = {Background Zika virus (ZIKV) has been linked to neonatal microcephaly. To characterize the spectrum of ZIKV disease in pregnancy, we followed patients in Rio de Janeiro to describe clinical manifestations in mothers and repercussions of acute ZIKV infection in fetuses. Methods We enrolled pregnant women in whom a rash had developed within the previous 5 days and tested blood and urine specimens for ZIKV by reverse-transcriptase-polymerase-chain-reaction assays. We followed the women prospectively and collected clinical and ultrasonographic data. Results A total of 88 women were enrolled from September 2015 through February 2016; of these 88 women, 72 (82{\%}) tested positive for ZIKV in blood, urine, or both. The timing of acute ZIKV infection ranged from 5 to 38 weeks of gestation. Predominant clinical features included pruritic descending macular or maculopapular rash, arthralgias, conjunctival injection, and headache; 28{\%} had fever (short-term and low-grade). Women who were positive for ZIKV were more likely than those who were negative for the virus to have maculopapular rash (44{\%} vs. 12{\%}, P=0.02), conjunctival involvement (58{\%} vs. 13{\%}, P=0.002), and lymphadenopathy (40{\%} vs. 7{\%}, P=0.02). Fetal ultrasonography was performed in 42 ZIKV-positive women (58{\%}) and in all ZIKV-negative women. Fetal abnormalities were detected by Doppler ultrasonography in 12 of the 42 ZIKV-positive women (29{\%}) and in none of the 16 ZIKV-negative women. Adverse findings included fetal deaths at 36 and 38 weeks of gestation (2 fetuses), in utero growth restriction with or without microcephaly (5 fetuses), ventricular calcifications or other central nervous system (CNS) lesions (7 fetuses), and abnormal amniotic fluid volume or cerebral or umbilical artery flow (7 fetuses). To date, 8 of the 42 women in whom fetal ultrasonography was performed have delivered their babies, and the ultrasonographic findings have been confirmed. Conclusions Despite mild clinical symptoms, ZIKV infection during pregnancy appears to be associated with grave outcomes, including fetal death, placental insufficiency, fetal growth restriction, and CNS injury.},
archivePrefix = {arXiv},
arxivId = {arXiv:1011.1669v3},
author = {Brasil, Patr{\'{i}}cia and Pereira, Jos{\'{e}} P. and Moreira, Maria Elisabeth and {Ribeiro Nogueira}, Rita M. and Damasceno, Luana and Wakimoto, Mayumi and Rabello, Renata S. and Valderramos, Stephanie G. and Halai, Umme-Aiman and Salles, Tania S. and Zin, Andrea A. and Horovitz, Dafne and Daltro, Pedro and Boechat, Marcia and {Raja Gabaglia}, Claudia and {Carvalho de Sequeira}, Patr{\'{i}}cia and Pilotto, Jos{\'{e}} H. and Medialdea-Carrera, Raquel and {Cotrim da Cunha}, Denise and {Abreu de Carvalho}, Liege M. and Pone, Marcos and {Machado Siqueira}, Andr{\'{e}} and Calvet, Guilherme A. and {Rodrigues Bai{\~{a}}o}, Ana E. and Neves, Elizabeth S. and {Nassar de Carvalho}, Paulo R. and Hasue, Renata H. and Marschik, Peter B. and Einspieler, Christa and Janzen, Carla and Cherry, James D. and {Bispo de Filippis}, Ana M. and Nielsen-Saines, Karin},
doi = {10.1056/NEJMoa1602412},
eprint = {arXiv:1011.1669v3},
file = {:Users/raphaelsaldanha/Library/Application Support/Mendeley Desktop/Downloaded/Brasil et al. - 2016 - Zika Virus Infection in Pregnant Women in Rio de Janeiro.pdf:pdf},
