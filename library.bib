@article{Shi2016a,
abstract = {Adjuvant chemotherapy (CTX) should be individualized to make the potential survival benefit and avoid potential harm for cancer patients. Our goal was to establish the computational approach for making personalized estimates of the survival benefit from adjuvant CTX. We developed Sub-Network based Ran-dom Forest classifier for predicting Chemotherapy Benefit (SNRFCB) based gene expression dataset of lung cancer. SNRFCB approach was then validated to independent test cohort for identifying chemotherapy responder cohorts and chemotherapy non-responder cohorts. SNRFCB involved the pre-selection of genes sub-network signatures based on the mutations and on protein-protein interactions data as well as the application of random forest algorithm to gene expression datasets. Lung cancer patients receiving adjuvant CTX were significantly associated with prolonged overall survival in the chemotherapy responder group (P=0.008), but was not beneficial in the chemotherapy non-responder group (P=0.657). Lung cancer squamous cell carcinoma (SQCC) subtype patients treated with adjuvant CTX were significantly associated with prolonged overall survival of the chemotherapy responder cohorts (P=0.024), but was not beneficial for the chemotherapy non-responder cohorts P=0.383). SNRFCB improved prediction performance as compared to the machine learning method support vector machine (SVM). To test the general applicability of the predictive model, we further applied SNRFCB approach on human breast cancer datasets and also observed superior performance. SNRFCB could provide recurrent probability for individual patient and identify which patient may benefit from adjuvant CTX in clinical trial.},
author = {Shi, Mingguang and He, Jianmin},
doi = {10.1039/C5MB00399G},
file = {:Users/raphaelsaldanha/Library/Application Support/Mendeley Desktop/Downloaded/Shi, He - 2016 - SNRFCB sub-network based random forest classifier for predicting chemotherapy benefit on survival for cancer treatment.pdf:pdf},
isbn = {1742-206X},
issn = {1742-206X},
journal = {Mol. BioSyst.},
pages = {1214--1223},
publisher = {Royal Society of Chemistry},
title = {{SNRFCB: sub-network based random forest classifier for predicting chemotherapy benefit on survival for cancer treatment}},
url = {http://xlink.rsc.org/?DOI=C5MB00399G},
volume = {12},
year = {2016}
}
