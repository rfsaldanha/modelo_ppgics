@article{Araujo2015b,
abstract = {Partimos do pressuposto de que os Grupos Tem{\'{a}}ticos nos congressos s{\~{a}}o, para al{\'{e}}m de espa{\c{c}}os de visibiliza{\c{c}}{\~{a}}o de uma produ{\c{c}}{\~{a}}o cient{\'{i}}fica, da articula{\c{c}}{\~{a}}o de processos de coopera{\c{c}}{\~{a}}o institucional, acad{\^{e}}mica e cient{\'{i}}fica, lugares de emerg{\^{e}}ncia de novos paradigmas e/ou cr{\'{i}}tica a paradigmas vigentes e – objeto aqui de nosso interesse espec{\'{i}}fico – de legitima{\c{c}}{\~{a}}o e consolida{\c{c}}{\~{a}}o dos campos organizados sobre uma denomina{\c{c}}{\~{a}}o. Nosso objetivo {\'{e}} tra{\c{c}}ar - de forma ainda preliminar – alguns contornos do cen{\'{a}}rio da pesquisa em Comunica{\c{c}}{\~{a}}o e Sa{\'{u}}de no Brasil e em Portugal, atrav{\'{e}}s dos trabalhos apresentados em congressos, especificamente nos grupos que tenham a Comunica{\c{c}}{\~{a}}o e Sa{\'{u}}de como tema principal de interesse. Foi examinada a produ{\c{c}}{\~{a}}o recente dos GTs "Comunica{\c{c}}{\~{a}}o e Sa{\'{u}}de" da Associa{\c{c}}{\~{a}}o Latinoamericana de Pesquisadores da Comunica{\c{c}}{\~{a}}o, "Comunica{\c{c}}{\~{a}}o e Sa{\'{u}}de" da Associa{\c{c}}{\~{a}}o Brasileira de Sa{\'{u}}de Coletiva, "Comunica{\c{c}}{\~{a}}o, Sa{\'{u}}de e Sociedade", do Congresso Brasileiro de Ci{\^{e}}ncias Sociais em Sa{\'{u}}de e "Informa{\c{c}}{\~{a}}o, Comunica{\c{c}}{\~{a}}o e Sa{\'{u}}de em Pa{\'{i}}ses Lus{\'{o}}fonos" do Congresso de Ci{\^{e}}ncias Sociais dos Pa{\'{i}}ses de L{\'{i}}ngua Portuguesa. Os resultados s{\~{a}}o descritivos, caracterizando-se o trabalho como um primeiro passo no sentido de uma configura{\c{c}}{\~{a}}o anal{\'{i}}tica do lugar dos Grupos de Comunica{\c{c}}{\~{a}}o e Sa{\'{u}}de na forma{\c{c}}{\~{a}}o do campo.},
author = {Ara{\'{u}}jo, Inesita S},
