isbn = {10.2105/AJPH.2010.202754},
issn = {15410048},
journal = {American journal of public health},
number = {4},
pages = {707--713},
pmid = {21330578},
title = {{Social network analysis of patient sharing among hospitals in Orange County, California.}},
volume = {101},
year = {2011}
}
