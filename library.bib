file = {:Users/raphaelsaldanha/Library/Application Support/Mendeley Desktop/Downloaded/S{\o}ndergaard, Andersen, Hj{\o}rland - 2005 - Documents and the communication of scientific and scholarly information Revising and updating.pdf:pdf},
