@article{Tauil1998,
abstract = {REFERENCE: TAU1L (P.L.) {\&} DANIEL-RIBEIRO (C.T.), 1998.-Some aspects of epidemiology and control of malaria in Brazil. Research and Reviews in ParasitoLogy, 58 (3-4): 163-167. SUMMARY: Malaria is still nowadays a major public health problem in Brazil. It is estimated that in the early 40s, approximately 20{\%} of the natio-nal population were infected each year. As a result of the fight against the disease, the number of malaria cases decreased over the years and the smallest number to-date was recorded in the 60s. From the mid-60s onwards, Brazil underwent a rapid and disorganized settlement process in the Amazon and this migratory movement led to a progressive increase in the number of reported cases. Although the main mosquito vector (Anophe-Les darlingi) is present in about 80{\%} of the country's total area, currently the incidence of malaria in Brazil is almost exclusively (more than 99{\%} of the cases) restricted to the region of the Amazon Basin, where a number of combined factors favour disease transmission and impair the use of stan-dard control procedures. Plasmodium vivax accounts for more than 75{\%} and P. falciparum for around 23{\%} of the cases. P. malariae is seldom ob-served and there are no records of autochthonous cases of P. ovale. The present difficulty in reducing economic and social risk factors that deter-mine the incidence of malaria in the Amazona Region render impracticable its eradication in the Region. As a result, and following recommendations of the Malaria Ministerial Conference held in 1992in Amsterdam, a malaria integrated control effort (as ajoint action on the part of the government and the population) directed towards the elimination or reduction of the risks of death or illness, is the new direction adopted by the Brazilian government in the fight against the disease. KEy WORDS: Brazil, malaria, epidemiology, control, transmission, Plasmodium falciparum, P. vivax, P. malariae.},
author = {Tauil, P and Daniel-Ribeiro, C},
file = {:Users/raphaelsaldanha/Library/Application Support/Mendeley Desktop/Downloaded/Tauil, Daniel-Ribeiro - 1998 - Some aspects of epidemiology and control of malaria in Brazil.pdf:pdf},
issn = {0034-9623},
journal = {Res Rev Parasitol},
number = {October},
pages = {163--167},
title = {{Some aspects of epidemiology and control of malaria in Brazil}},
url = {http://scholar.google.com/scholar?hl=en{\&}btnG=Search{\&}q=intitle:Some+aspects+of+epidemiology+and+control+of+malaria+in+Brazil{\#}0},
volume = {58},
year = {1998}
}
