@inproceedings{Kostkova2013,
abstract = {The exponentially increasing str eam of real time big data produced by Web 2.0 Internet and mobile networks created radically new interdisciplinary challenges for public health and computer science. Traditional pub lic health disease surveillance systems have to utilize the potential created by new situation- aware realtime signals from social media, mobile/sensor networks and citizens' participatory surveillance systems providing invaluable free realtime event-based signals for epidemic intelligence. However, rather than improving existing isolated systems, an integrated solution bringing together existing epidemic intelligence systems scanning news media (e.g., GPHIN, MedISys) with real-time social media intelligence (e.g., Twitter, participatory system s) is required to substantially improve and automate early wa rning, outbreak detection and preparedness operations. Howeve r, automatic monitoring and novel verification methods for these multichannel event-based real time signals has to be inte grated with traditional case-based surveillance systems from microbiological laboratories and clinical reporting. Finally, the system needs effectively support coordination of epidemiological teams, risk communication with citizens and implementation of prevention measures. However, from computational pe rspective, signal detection, analysis and verification of very high noise realtime big data provide a number of interdiscip linary challenges for computer science. Novel approaches integrating current systems into a digital public health dashboard can enhance signal verification methods and automate the pro cesses assisting public health experts in providing better inform ed and more timely response. In this paper, we describe the roadmap to such a system, components of an integrated public health surveillance services and computing challenges to be resolved to create an integrated real world solution.},
address = {New York, New York, USA},
author = {Kostkova, Patty},
booktitle = {Proceedings of the 22nd International Conference on World Wide Web - WWW '13 Companion},
doi = {10.1145/2487788.2488024},
file = {:Users/raphaelsaldanha/Library/Application Support/Mendeley Desktop/Downloaded/Kostkova - 2013 - A roadmap to integrated digital public health surveillance.pdf:pdf},
isbn = {9781450320382},
keywords = {epidemic intelligence,outbreak detection,risk},
pages = {687--694},
publisher = {ACM Press},
title = {{A roadmap to integrated digital public health surveillance}},
url = {http://www2013.org/companion/p687.pdf http://dl.acm.org/citation.cfm?doid=2487788.2488024},
year = {2013}
}
