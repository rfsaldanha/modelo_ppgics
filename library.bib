journal = {Malaria journal},
keywords = {Coinfection,French Guiana,Humans,Malaria, Falciparum,Malaria, Vivax,Microscopy,Plasmodium falciparum,Plasmodium vivax,Real-Time Polymerase Chain Reaction,epidemiology,isolation {\&} purification,parasitology},
pages = {446},
