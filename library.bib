@article{DeAraujo2016a,
abstract = {Background The microcephaly epidemic, which started in Brazil in 2015, was declared a Public Health Emergency of International Concern by WHO in 2016. We report the preliminary results of a case-control study investigating the association between microcephaly and Zika virus infection during pregnancy. Methods We did this case-control study in eight public hospitals in Recife, Brazil. Cases were neonates with microcephaly. Two controls (neonates without microcephaly), matched by expected date of delivery and area of residence, were selected for each case. Serum samples of cases and controls and cerebrospinal fluid samples of cases were tested for Zika virus-specific IgM and by quantitative RT-PCR. Laboratory-confirmed Zika virus infection during pregnancy was defined as detection of Zika virus-specific IgM or a positive RT-PCR result in neonates. Maternal serum samples were tested by plaque reduction neutralisation assay for Zika virus and dengue virus. We estimated crude odds ratios (ORs) and 95{\%} CIs using a median unbiased estimator for binary data in an unconditional logistic regression model. We estimated ORs separately for cases with and without radiological evidence of brain abnormalities. Findings Between Jan 15, 2016, and May 2, 2016, we prospectively recruited 32 cases and 62 controls. 24 (80{\%}) of 30 mothers of cases had Zika virus infection compared with 39 (64{\%}) of 61 mothers of controls (p=0{\textperiodcentered}12). 13 (41{\%}) of 32 cases and none of 62 controls had laboratory-confirmed Zika virus infection; crude overall OR 55{\textperiodcentered}5 (95{\%} CI 8{\textperiodcentered}6–∞); OR 113{\textperiodcentered}3 (95{\%} CI 14{\textperiodcentered}5–∞) for seven cases with brain abnormalities; and OR 24{\textperiodcentered}7 (95{\%} CI 2{\textperiodcentered}9–∞) for four cases without brain abnormalities. Interpretation Our data suggest that the microcephaly epidemic is a result of congenital Zika virus infection. We await further data from this ongoing study to assess other potential risk factors and to confirm the strength of association in a larger sample size. Funding Brazilian Ministry of Health, Pan American Health Organization, and Enhancing Research Activity in Epidemic Situations.},
author = {de Ara{\'{u}}jo, Thalia Velho Barreto and Rodrigues, Laura Cunha and {de Alencar Ximenes}, Ricardo Arraes and {de Barros Miranda-Filho}, Dem{\'{o}}crito and Montarroyos, Ulisses Ramos and de Melo, Ana Paula Lopes and Valongueiro, Sandra and de Albuquerque, Maria de F{\'{a}}tima Pessoa Milit{\~{a}}o and Souza, Wayner Vieira and Braga, Cynthia and Filho, Sinval Pinto Brand{\~{a}}o and Cordeiro, Marli Ten{\'{o}}rio and Vazquez, Enrique and {Di Cavalcanti Souza Cruz}, Danielle and Henriques, Cl{\'{a}}udio Maierovitch Pessanha and Bezerra, Luciana Caroline Albuquerque and {da Silva Castanha}, Priscila Mayrelle and Dhalia, Rafael and Marques-J{\'{u}}nior, Ernesto Torres Azevedo and Martelli, Celina Maria Turchi},
doi = {10.1016/S1473-3099(16)30318-8},
file = {:Users/raphaelsaldanha/Library/Application Support/Mendeley Desktop/Downloaded/de Ara{\'{u}}jo et al. - 2016 - Association between Zika virus infection and microcephaly in Brazil, January to May, 2016 preliminary report.pdf:pdf},
