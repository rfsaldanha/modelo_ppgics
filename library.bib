@article{Schwartz1999a,
abstract = {This paper considers ways of thinking about causes and prevention that could guide epidemiology beyond the present era. Discontent with modem epidemiology, in the face of its substantial achievements, is taken as a sign that the guiding principles of the discipline warrant reconsideration. To begin this task, current practices are placed within an historical perspective, in a review of the dominant ideas of successive eras in epidemiology. Then the premises and constraints of the present era of chronic disease epidemiology, with its risk factor paradigm, are specified. Finally, elements of a causal paradigm for an emerging era are proposed. This paradigm encourages thinking about causes at multiple levels of organization and within the historical context of both societies and individuals. The proposed approach aims to preserve and build on the contributions of past eras, as well as the present one.},
author = {Schwartz, S. and Susser, E. and Susser, M.},
doi = {10.1146/annurev.publhealth.20.1.15},
file = {:Users/raphaelsaldanha/Downloads/annurev.publhealth.20.1.15.pdf:pdf},
issn = {0163-7525},
journal = {Annual Review of Public Health},
keywords = {Critiques of epidemiology,Eco- epidemiology,History,Methods,Multilevel epidemiology,Paradigms},
month = {may},
number = {1},
pages = {15--33},
pmid = {10352847},
title = {{A Future for Epidemiology?}},
url = {http://www.annualreviews.org/doi/10.1146/annurev.publhealth.20.1.15},
volume = {20},
year = {1999}
}
@article{Mooney2018,
abstract = {The digital world is generating data at a staggering and still increasing rate. While these “big data” have unlocked novel opportunities to understand public health, they hold still greater potential for research and practice. This review explores several key issues that have arisen around big data. First, we propose a taxonomy of sources of big data to clarify terminology and identify threads common across some subtypes of big data. Next, we consider common public health research and practice uses for big data, including surveillance, hypothesis-generating research, and causal inference, while exploring the role that machine learning may play in each use. We then consider the ethical implications of the big data revolution with particular emphasis on maintaining appropriate care for privacy in a world in which technology is rapidly changing social norms regarding the need for (and even the meaning of) privacy. Finally, we make suggestions regarding structuring teams and training to succeed in working with big data in research and practice.},
author = {Mooney, Stephen J. and Pejaver, Vikas},
doi = {10.1146/annurev-publhealth-040617-014208},
