@article{Christovao1979,
abstract = {A literatura informal, especialmente o conjunto das comunica{\c{c}}{\~{o}}es apresentadas em reuni{\~{o}}es cientificas representa uma parte significativa da estrutura da comunica{\c{c}}{\~{a}}o cient{\'{i}}fica, devendo uma parcela deste conjunto, filtrar-se at{\'{e}} a literatura super-formal (revis{\~{o}}es da literatura). Numa parcela do conjunto filtrado deve residir a frente de pesquisa. Tais premissas s{\~{a}}o constatadas medindo-se a inclus{\~{a}}o das comunica{\c{c}}{\~{o}}es na literatura de revis{\~{a}}o, utilizando-se a vari{\'{a}}vel autor. Atrav{\'{e}}s da manipula{\c{c}}{\~{a}}o dos dados referentes {\`{a}} {\'{a}}rea Ci{\^{e}}ncia da Informa{\c{c}}{\~{a}}o e contidos nas fontes LISA e ARIST (1961 - 1977), identifica-se um m{\'{e}}todo de filtragem (filtro de qualidade) da literatura cient{\'{i}}fica. Este m{\'{e}}todo {\'{e}} acionado pela pr{\'{o}}pria engrenagem do sistema de comunica{\c{c}}{\~{a}}o cientifica permitindo assim, a identifica{\c{c}}{\~{a}}o da frente de pesquisa.},
