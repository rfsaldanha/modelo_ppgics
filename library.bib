keywords = {Big data,Data ethnics,Data quality and error,Spatial information sciences},
pages = {134--142},
publisher = {International Society for Photogrammetry and Remote Sensing, Inc. (ISPRS)},
title = {{Rethinking big data: A review on the data quality and usage issues}},
url = {http://dx.doi.org/10.1016/j.isprsjprs.2015.11.006},
volume = {115},
year = {2015}
