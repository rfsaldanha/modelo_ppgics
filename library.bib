journal = {Malaria journal},
keywords = {Climate,French Guiana,French Guiana: epidemiology,Humans,Incidence,Malaria, Vivax,Malaria, Vivax: epidemiology,Plasmodium vivax,Plasmodium vivax: isolation {\&} purification,Rivers,Rural Population},
pages = {26},
pmid = {21294884},
title = {{Influence of climate and river level on the incidence of malaria in Cacao, French Guiana.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3042423{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {10},
