issn = {01023616},
journal = {Revista Brasileira de Ortopedia},
keywords = {Accident prevention,Acidentes de tr{\^{a}}nsito,Epidemiologia,Epidemiology,Preven{\c{c}}{\~{a}}o de acidentes,Traffic accidents},
month = {nov},
