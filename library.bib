@article{Santos2006,
abstract = {Over the last decade, the number of homicides in Porto Alegre has increased to the point where external causes are now the main group of causes of death in the 5-34-year age group. Preventing these deaths depends fundamentally on identifying factors related to excess violence in population groups. The overall aim of this study is to analyse the spatial distribution of homicide victims by place of residence in Porto Alegre, the capital of the southernmost Brazilian State of Rio Grande do Sul, in 1996, in order to identify and understand the socio-spatial context. Demographic and socioeconomic indicators based on the 1991 census and 1996 population count were used to build a multivariate classification characterizing the 1851 census tracts. Homicides occurring in 1996 were located using the municipality's Geographic Information System. Four socioeconomic groups were identified, mainly differentiated by housing indicators. Small areas on the urban periphery in which slums (favelas) are concentrated presented higher homicide rates. Homicide rates were lower in the two groups with higher income and educational level. The second step was to classify the census tracts according to the homicide indicator. In this case, areas were differentiated by the number of household inhabitants per room, income, schooling, and median age. We conclude that the multivariate socioeconomic classification presents a limited capacity to identify populations exposed to homicides, suggesting that socioeconomic conditions themselves do not determine violent behaviour. On the other hand, the spatial methods allowed us to identify small areas where deaths are concentrated and whose populations should receive special attention in planning measures to prevent violent deaths. {\textcopyright} 2004 Elsevier Ltd. All rights reserved.},
author = {Santos, Simone M. and Barcellos, Christovam and Carvalho, Marilia S{\'{a}}},
doi = {10.1016/j.healthplace.2004.08.009},
file = {:Users/raphaelsaldanha/Library/Application Support/Mendeley Desktop/Downloaded/Santos, Barcellos, Carvalho - 2006 - Ecological analysis of the distribution and socio-spatial context of homicides in Porto Alegre, Bra.pdf:pdf},
isbn = {1353-8292 (Print)$\backslash$r1353-8292 (Linking)},
issn = {13538292},
journal = {Health and Place},
keywords = {Geographic information system,Homicide,Quality of life indicators,Spatial analysis,Violence},
number = {1},
pages = {38--47},
pmid = {16243679},
title = {{Ecological analysis of the distribution and socio-spatial context of homicides in Porto Alegre, Brazil}},
volume = {12},
year = {2006}
}
