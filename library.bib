@article{Domingos1999,
abstract = {Many KDD systems incorporate an implicit or explicit preference for simpler models, but this use of "Occam's razor" has been strongly criticized by several authors (e.g., Schaffer, 1993; Webb, 1996). This controversy arises partly because Occam's razor has been interpreted in two quite different ways. The first interpretation (simplicity is a goal in itself) is essentially correct, but is at heart a preference for more comprehensible models. The second interpretation (simplicity leads to greater accuracy) is much more problematic. A critical review of the theoretical arguments for and against it shows that it is unfounded as a universal principle, and demonstrably false. A review of empirical evidence shows that it also fails as a practical heuristic. This article argues that its continued use in KDD risks causing significant opportunities to be missed, and should therefore be restricted to the comparatively few applications where it is appropriate. The article proposes and reviews the use of domain constraints as an alternative for avoiding overfilling, and examines possible methods for handling the accuracy-comprehensibility trade-off. {\textcopyright} 1999 Kluwer Academic Publishers.},
author = {Domingos, Pedro},
doi = {10.1023/A:1009868929893},
file = {:Users/raphaelsaldanha/Library/Application Support/Mendeley Desktop/Downloaded/Domingos - 1999 - The role of Occam's Razor in knowledge discovery.pdf:pdf},
issn = {13845810},
journal = {Data Mining and Knowledge Discovery},
keywords = {Comprehensible models,Domain knowledge,Model selection,Multiple comparisons,Overfilling},
number = {4},
pages = {409--425},
title = {{The role of Occam's Razor in knowledge discovery}},
volume = {3},
year = {1999}
}
