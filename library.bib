@article{DenBroeder2016a,
abstract = {Stakeholder engagement in Environmental Impact Assessment (EIA) and$\backslash$nHealth Impact Assessment (HIA) provides opportunities for inclusive$\backslash$nenvironmental decision-making contributing to the attainment of$\backslash$nagreement about the potential environmental and health impacts of a$\backslash$nplan. A case evaluation of stakeholder engagement was carried out to$\backslash$nassess its effect in terms of consensus-building. The case consisted in$\backslash$ntwo health impact scoping workshops engaging 20 stakeholders:$\backslash$npolicy-makers, experts and residents. A Participatory Action Research$\backslash$napproach was adopted. Methods included observation, semi-structured$\backslash$nquestionnaires and interviews. Analysis methods consisted of several$\backslash$ncoding rounds, in-depth reading and discussion of Atlas.ti output$\backslash$nreports, as well as studying questionnaire results. Participants$\backslash$nreported a broadening of perspectives on health in relation to the$\backslash$nenvironment and attainment of shared perspectives. Still, meaningful$\backslash$ndifferences remained, indicating that joint learning experiences, trust$\backslash$nand mutual respect created a sense of consensus' rather than a joint$\backslash$nview on the issues at stake. To avoid disappointment and conflict in$\backslash$nlater project development, explicit acknowledgment and acceptance of$\backslash$ndisagreements should be included as a ground rule in future stakeholder$\backslash$nengagement processes.},
author = {den Broeder, Lea and Chung, Kai Yin and Geelen, Loes and Scholtes, Monique and Schuit, Albertina Jantine and Wagemakers, Annemarie},
doi = {10.1080/14615517.2016.1176413},
file = {:Users/raphaelsaldanha/Library/Application Support/Mendeley Desktop/Downloaded/den Broeder et al. - 2016 - We are all experts! Does stakeholder engagement in health impact scoping lead to consensus A Dutch case stud.pdf:pdf},
isbn = {14615517},
issn = {1461-5517},
journal = {Impact Assessment and Project Appraisal},
keywords = {Stakeholder engagement,environmental impact assessment,health,scoping},
number = {4},
pages = {294--305},
publisher = {Taylor {\&} Francis},
title = {{We are all experts! Does stakeholder engagement in health impact scoping lead to consensus? A Dutch case study}},
url = {https://www.tandfonline.com/doi/full/10.1080/14615517.2016.1176413},
volume = {34},
year = {2016}
}
