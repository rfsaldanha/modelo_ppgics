pages = {450--457},
title = {{The role of Brazilian National Health Information Systems in assessing the impact of Zika virus outbreak}},
url = {http://www.scielo.br/scielo.php?script=sci{\_}arttext{\&}pid=S0037-86822017000400450{\&}lng=en{\&}tlng=en},
