@article{Lessa2000,
abstract = {A vigil{\^{a}}ncia epidemiol{\'{o}}gica das doen{\c{c}}as de notifica{\c{c}}{\~{a}}o compuls{\'{o}}ria tem como principal fonte de informa{\c{c}}{\~{a}}o os servi{\c{c}}os de sa{\'{u}}de, sobretudo ambulatorial, atrav{\'{e}}s do preenchimento das fichas de notifica{\c{c}}{\~{a}}o e investiga{\c{c}}{\~{a}}o do Sistema de Informa{\c{c}}{\~{a}}o de Agravos de Notifica{\c{c}}{\~{a}}o - SINAN. Os estabelecimentos de sa{\'{u}}de hospitalares, na sua maioria, em fun{\c{c}}{\~{a}}o de sua complexidade e outros determinantes, notificam pouco suas interna{\c{c}}{\~{o}}es, atrav{\'{e}}s do SINAN, mesmo com rela{\c{c}}{\~{a}}o a doen{\c{c}}as de notifica{\c{c}}{\~{a}}o compuls{\'{o}}ria. Embora sem a implanta{\c{c}}{\~{a}}o desse sistema de informa{\c{c}}{\~{a}}o, todos os hospitais conveniados com o Sistema {\'{U}}nico de Sa{\'{u}}de, disp{\~{o}}em do Sistema de Informa{\c{c}}{\~{o}}es Hospitalares - SIH, possibilitando-lhes atuar como uma fonte de informa{\c{c}}{\~{a}}o complementar para subsidiar as a{\c{c}}{\~{o}}es de vigil{\^{a}}ncia epidemiol{\'{o}}gica. Na tentativa de incrementar a{\c{c}}{\~{o}}es de vigil{\^{a}}ncia epidemiol{\'{o}}gica, o Centro Nacional de Epidemiologia - CENEPI vem estimulando estudos de novas metodologias. O uso do SIH para subsidiar a{\c{c}}{\~{o}}es de vigil{\^{a}}ncia epidemiol{\'{o}}gica tem se comportado como uma importante tecnologia, principalmente pela sua agilidade e f{\'{a}}cil acesso. Este manual tem como objetivo instrumentalizar os servi{\c{c}}os de sa{\'{u}}de para a explora{\c{c}}{\~{a}}o do SIH no enfoque da vigil{\^{a}}ncia epidemiol{\'{o}}gica, iniciando com uma discuss{\~{a}}o mais te{\'{o}}rica sobre aspectos conceituais da informa{\c{c}}{\~{a}}o e sua interface com a epidemiologia. Em seguida, aborda o Sistema de Informa{\c{c}}{\~{o}}es Hospitalares quanto a descri{\c{c}}{\~{a}}o, objetivos, instrumentos, informatiza{\c{c}}{\~{a}}o, fluxo, limites e possibilidades. Visando oferecer subs{\'{i}}dios para explora{\c{c}}{\~{a}}o das bases de dados do SIH, realiza-se uma discuss{\~{a}}o sobre o processo de investiga{\c{c}}{\~{a}}o dos problemas de sa{\'{u}}de, a constru{\c{c}}{\~{a}}o de indicadores e uma introdu{\c{c}}{\~{a}}o ao banco de dados, sua descri{\c{c}}{\~{a}}o e seu acesso. Por fim, este manual serve de apoio para iniciar o processo de explora{\c{c}}{\~{a}}o dos arquivos gerados pelo SIH, trabalhando passo a passo para a obten{\c{c}}{\~{a}}o de informa{\c{c}}{\~{o}}es epidemiol{\'{o}}gicas das interna{\c{c}}{\~{o}}es hospitalares por doen{\c{c}}as de notifica{\c{c}}{\~{a}}o compuls{\'{o}}ria.},
author = {Lessa, F{\'{a}}bio Jos{\'{e}} Delgado and Mendes, Ant{\^{o}}nio da Cruz Gouveia and Farias, Sidney Feitosa and de S{\'{a}}, Dom{\'{i}}cio Aur{\'{e}}lio and Duarte, Petra Oliveira and de {Melo Filho}, Djalma Agripino},
doi = {10.5123/S0104-16732000000500001},
file = {:Users/raphaelsaldanha/Library/Application Support/Mendeley Desktop/Downloaded/Lessa et al. - 2000 - Novas metodologias para vigil{\^{a}}ncia epidemiol{\'{o}}gica uso do Sistema de Informa{\c{c}}{\~{o}}es Hospitalares - SIHSUS.pdf:pdf},
issn = {0104-1673},
journal = {Informe Epidemiol{\'{o}}gico do Sus},
pages = {3--27},
title = {{Novas metodologias para vigil{\^{a}}ncia epidemiol{\'{o}}gica: uso do Sistema de Informa{\c{c}}{\~{o}}es Hospitalares - SIH/SUS}},
url = {http://scielo.iec.pa.gov.br/scielo.php?script=sci{\_}arttext{\&}pid=S1679-16732000000500001{\&}lng=en{\&}nrm=iso{\&}tlng=en},
