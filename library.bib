isbn = {1581138889},
journal = {Proceedings of the 2004 ACM SIGKDD international conference on Knowledge discovery and data mining - KDD '04},
keywords = {also address the question,clusters,could,if they can explain,monte carlo,search,space-time region,spatial scan statis-,the evaluation must,the excessive cases,tic,whether the excessive cases},
pages = {587},
title = {{On detecting space-time clusters}},
