journal = {Accident; analysis and prevention},
keywords = {Accidents,Adolescent,Adult,Econometric,Humans,Income,Income: statistics {\&} numerical data,Middle Aged,Models,Motorcycles,Traffic,Traffic: economics,Traffic: legislation {\&} jurisprudence,Traffic: mortality,Young Adult},
month = {mar},
number = {2},
pages = {234--40},
pmid = {19245880},
title = {{Factors associated with the relationship between motorcycle deaths and economic growth.}},
url = {http://www.sciencedirect.com/science/article/pii/S0001457508002200},
volume = {41},
year = {2009}
}
