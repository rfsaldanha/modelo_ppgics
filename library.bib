keywords = {Big data,bioinformatics,health informatics,medical imaging,medical informatics,precision medicine,sensor informatics,social health},
number = {4},
pages = {1193--1208},
publisher = {IEEE},
title = {{Big Data for Health}},
volume = {19},
year = {2015}
}
