@article{Wang2016,
abstract = {Abstract To date, health care industry has not fully grasped the potential benefits to be gained from big data analytics. While the constantly growing body of academic research on big data analytics is mostly technology oriented, a better understanding of the strategic implications of big data is urgently needed. To address this lack, this study examines the historical development, architectural design and component functionalities of big data analytics. From content analysis of 26 big data implementation cases in healthcare, we were able to identify five big data analytics capabilities: analytical capability for patterns of care, unstructured data analytical capability, decision support capability, predictive capability, and traceability. We also mapped the benefits driven by big data analytics in terms of information technology (IT) infrastructure, operational, organizational, managerial and strategic areas. In addition, we recommend five strategies for healthcare organizations that are considering to adopt big data analytics technologies. Our findings will help healthcare organizations understand the big data analytics capabilities and potential benefits and support them seeking to formulate more effective data-driven analytics strategies.},
author = {Wang, Yichuan and Kung, LeeAnn and Byrd, Terry Anthony},
doi = {http://dx.doi.org/10.1016/j.techfore.2015.12.019},
file = {:Users/raphaelsaldanha/Library/Application Support/Mendeley Desktop/Downloaded/Wang, Kung, Byrd - 2016 - Big data analytics Understanding its capabilities and potential benefits for healthcare organizations.pdf:pdf},
issn = {0040-1625},
journal = {Technological Forecasting and Social Change},
keywords = {Big data analytics,Big data analytics architecture,Big data analytics capabilities,Business value of information technology (IT),Health care},
pages = {11},
publisher = {Elsevier Inc.},
title = {{Big data analytics: Understanding its capabilities and potential benefits for healthcare organizations}},
url = {http://www.sciencedirect.com/science/article/pii/S0040162516000500},
year = {2016}
}
